{
  "name": "polaris-ui",
  "author": "Markus Ketzer",
  "description": "A UI for managing Polaris Workspaces",
  "version": "0.0.0",
  "main": "electron/main.js",
  "scripts": {
    "start": "vite",
    "dev": "vite",

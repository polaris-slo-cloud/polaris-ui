    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.17",
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.3",
    "electron": "^17.1.2",
    "electron-builder": "^22.14.13",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
